        <Button
			Name="UserCubeButton"
			Width="184" Height="34"
			HorizontalAlignment="Left" VerticalAlignment="Top"
			Margin="858,398,0,0" Background="#FFF0F0F0"
			FontFamily="Arial" FontSize="14" FontWeight="Bold"
			Content="Задать куб"
			Click="UserCubeButtonClick" RenderTransformOrigin="0.51,-1.122" />
