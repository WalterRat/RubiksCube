			Name="SetColorHeading"
			Width="240" Height="26"
			HorizontalAlignment="Left" VerticalAlignment="Top"
			Margin="831,268,0,0" Background="#FFFFFFFF"
			FontFamily="Arial" FontSize="14" FontWeight="Bold"
